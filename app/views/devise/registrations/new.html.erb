</div>

<div id="myModal" class="modal fade rounded mr-2" tabindex="-1" role="alert">
  <div class="modal-dialog" role="document">
    <div class="modal-content" style="color: blueviolet;">
      <div class="modal-header">
        <p class="modal-title std-font-grey">Há campos com erros</p>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
    </div>
  </div>
</div>

<script>
  //Máscara
  $(document).ready(function () {
    $('#user_cpf').mask('000.000.000-00');
    $('#user_cep').mask('00000-000');
    $('#user_cnpj').mask('00.000.000/0000-00');
    $('#user_documento').mask('0#');
  });

  //Campos Nome e Sobrenome aceitando somente letras, acentos e espaços
  $(document).ready(function () {
    $("#user_nome").on("input", function () {
      var regexp = /[^A-Za-záàâãéèêíïóôõöúçñÁÀÂÃÉÈÍÏÓÔÕÖÚÇÑ\'\˜\^ ]/;
      if (this.value.match(regexp)) {
        $(this).val(this.value.replace(regexp, ''));
      }
    });
    $("#user_sobrenome").on("input", function () {
      var regexp = /[^A-Za-záàâãéèêíïóôõöúçñÁÀÂÃÉÈÍÏÓÔÕÖÚÇÑ\'\˜\^ ]/;
      if (this.value.match(regexp)) {
        $(this).val(this.value.replace(regexp, ''));
      }
    });
  })

  //Remove datas futuras para data de emissão
  $(document).ready(function () {
    var today = new Date();
    var dd = today.getDate();
    var mm = today.getMonth() + 1;
    var yyyy = today.getFullYear();
    if (dd < 10) {
      dd = '0' + dd
    }
    if (mm < 10) {
      mm = '0' + mm
    }
    today = yyyy + '-' + mm + '-' + dd;
    $("#user_data_emissao").attr("max", today);
  });
  $(document).ready(function () {
    $('select').attr("class", "form-control");
  });
  //Altera cor do campo CPF se for um formato inválido
  $(document).ready(function () {
    $('#user_cpf').change(function () {
      var filtrocpf = /\d{3}\.\d{3}\.\d{3}\-\d{2}/;
      if (!(filtrocpf.test($(this).val()))) {
        $(this).attr('style', 'border-color: #A94342;');
      } else {
        $(this).attr('style', 'border-color: #586871');
      }
    })
  });
  //Troca entre o Form de PF e PJ
  $(document).ready(function () {
    $('#user_pf_pj').change(function () {
      if ($('#user_pf_pj option:selected').val() == 'PF') {
        $('#pj').hide();
        $('#pf').show();
        $('#user_nome_fantasia').val('');
        $('#user_razao_social').val('');
        $('#user_cnpj').val('');
      } else if ($('#user_pf_pj option:selected').val() == 'PJ') {
        $('#pf').hide();
        $('#pj').show();
        $('#user_nome').val('');
        $('#user_sobrenome').val('');
        $('#user_cpf').val('');
        $('#user_documento').val('');
        $('#user_orgao_emissor').val('');
        $('#user_estado').val('');
      }
    });
  })
  //Completar endereço via CEP
  $(document).ready(function () {
    function limpa_formulário_cep() {
      // Limpa valores do formulário de cep.
      $("#user_logradouro").val("");
      $("#user_bairro").val("");
      $("#user_cidade").val("");
      $("#user_estado").val("");
    }
    //Quando o campo cep perde o foco.
    $("#user_cep").blur(function () {
      //Nova variável "cep" somente com dígitos.
      var cep = $(this).val().replace(/\D/g, '');
      //Verifica se campo cep possui valor informado.
      if (cep != "") {
        //Expressão regular para validar o CEP.
        var validacep = /^[0-9]{8}$/;
        //Valida o formato do CEP.
        if (validacep.test(cep)) {
          //Preenche os campos com "..." enquanto consulta webservice.
          $("#user_logradouro").val("...");
          $("#user_bairro").val("...");
          $("#user_cidade").val("...");
          //Consulta o webservice viacep.com.br/
          $.getJSON("https://viacep.com.br/ws/" + cep + "/json/?callback=?", function (dados) {
            if (!("erro" in dados)) {
              //Atualiza os campos com os valores da consulta.
              $("#user_logradouro").val(dados.logradouro);
              $("#user_bairro").val(dados.bairro);
              $("#user_cidade").val(dados.localidade);
              $("#user_estado").val(dados.uf);
            } //end if.
            else {
              //CEP pesquisado não foi encontrado.
              limpa_formulário_cep();
            }
          });
        } //end if.
        else {
          //cep é inválido.
          limpa_formulário_cep();
          alert("Formato de CEP inválido.");
        }
      } //end if.
      else {
        //cep sem valor, limpa formulário.
        limpa_formulário_cep();
      }
    });
  });
  //Vai para próximo text-field quando aperta Enter
  $('input').keypress(function (event) {
    var keycode = (event.keyCode ? event.keyCode : event.which);
    if (keycode == '13') {
      event.preventDefault();
      var $this = $(event.target);
      var index = parseFloat($this.attr('dataid'));
      $('[dataid="' + (index + 1).toString() + '"]').focus();
    }
  });
  //Navegação cadastros
  $(document).ready(function () {
    continuarClicked = false;

    //Valida campos em tempo real
    $('input').keyup(function () {
      if (continuarClicked)
        validateFields();
    });
    $('select').change(function () {
      if (continuarClicked)
        validateFields();
    });
    $('input').change(function () {
      if (continuarClicked)
        validateFields();
    });

    //Página PF PJ
    $('#btnNext1').click(function () {
      if ($('i').is(":visible") == false && continuarClicked) {
        $('#cadastro1').hide();
        $('#cadastro2').removeAttr('hidden');
        $('#cadastro2').show();
        $('#user_cep').focus();
        $('.progress-bar').css('width', 66 + '%').attr('aria-valuenow', 66);
        continuarClicked = false;
      }
      else {
        $('#myModal').modal({ show: true });
        validateFields();
      }
    });
    //Página Endereço
    $('#btnBack1').click(function () {
      $('#cadastro1').show();
      $('#cadastro2').hide();
      $('#user_nome').focus();
      $('.progress-bar').css('width', 33 + '%').attr('aria-valuenow', 33);
      continuarClicked = true;
    });
    $('#btnNext2').click(function () {
      if ($('i').is(":visible") == false && continuarClicked) {
        $('#cadastro2').hide();
        $('#cadastro3').removeAttr('hidden');
        $('#cadastro3').show();
        $('#user_email').focus();
        $('.progress-bar').css('width', 100 + '%').attr('aria-valuenow', 100);
        continuarClicked = false;
      }
      else {
        $('#myModal').modal({ show: true });
        validateFields();
      }
    });
    //Página E-mail Senha
    $('#btnBack2').click(function () {
      $('#cadastro2').show();
      $('#cadastro3').hide();
      $('#user_cep').focus();
      $('.progress-bar').css('width', 66 + '%').attr('aria-valuenow', 66);
      continuarClicked = true;
    });
    $('#btnCadastrar').click(function () {
      if ($('i').is(":visible") == false && continuarClicked) {
        //Submit cadastro
      }
      else {
        $('#myModal').modal({ show: true });
        validateFields();
      }
    });
    //Validações
    function validateFields() {
      continuarClicked = true;

      //Página PF PJ
      if ($('#cadastro1').is(":visible")) {
        //Checa campos vazios PF
        if ($('#user_pf_pj option:selected').val() == 'PF') {
          checkBlank($('#user_nome'), $('#alertNome'));
          checkBlank($('#user_sobrenome'), $('#alertSobrenome'));
          checkBlank($('#user_cpf'), $('#alertCPF'));
          checkBlank($('#user_documento'), $('#alertDoc'));
          checkBlank($('#user_orgao_emissor'), $('#alertOrgEms'));
          checkBlank($('#user_estado_emissor'), $('#alertEstEms'));
          checkBlank($('#user_data_emissao'), $('#alertDatEms'));
        }
        //Checa campos vazios PJ
        else {
          checkBlank($('#user_nome_fantasia'), $('#alertNomeFan'));
          checkBlank($('#user_razao_social'), $('#alertRazSoc'));
          checkBlank($('#user_cnpj'), $('#alertCNPJ'));
        }
        $('#alertCPF').tooltip({ 'trigger': 'hover', 'title': 'CPF inválido' });
      }
      //Página Endereço
      else if ($('#cadastro2').is(":visible")) {
        checkBlank($('#user_cep'), $('#alertCEP'));
        checkBlank($('#user_logradouro'), $('#alertLogr'));
        checkBlank($('#user_numero'), $('#alertNum'));
        checkBlank($('#user_bairro'), $('#alertBair'));
        checkBlank($('#user_cidade'), $('#alertCid'));
        checkBlank($('#user_estado'), $('#alertEst'));
      }
      //Página E-mail Senha
      else if ($('#cadastro3').is(":visible")) {
        checkBlank($('#user_email'), $('#alertEmail'));
        checkBlank($('#email_confirmation'), $('#alertEmailConf'));
        checkBlank($('#user_password'), $('#alertPW'));
        checkBlank($('#user_password_confirmation'), $('#alertPWConf'));
      }
    }
    //Checa campos vazios
    function checkBlank(field, alert) {
      if (field.val().replace(/^\s+|\s+$/g, "").length == 0) {
        alert.show();
        alert.tooltip({ 'trigger': 'hover', 'title': 'Campo em branco' });
        field.attr('style', 'border-color: #A94342;');
      }
      else if (alert.is(":visible")) {
        alert.hide();
        field.attr('style', 'border-color: #586871;');
      }
    }
  });


